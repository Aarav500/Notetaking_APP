# NestJS Backend Environment (Example)
# Database configuration (sqlite by default for local/dev)
DB_TYPE=sqlite
SQLITE_DB=dev.sqlite
DB_SYNCHRONIZE=true

# JWT configuration
JWT_SECRET=dev_secret_change_me
JWT_EXPIRES_IN=1d

# Reset password token TTL (ms)
RESET_TOKEN_TTL_MS=900000

# Optional Oracle wallet directory (stub for K8s volume mount)
# ORACLE_WALLET_DIR=/opt/oracle/wallet
